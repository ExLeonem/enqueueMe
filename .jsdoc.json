{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["./commands", "./core"],
        "exclude": ["./assets", "./coverage", "./data", "./node_modules", "./tests"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "referenceTitle": "QueueBot API",
        "disableSort": false,
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true,
        "template": "./node_modules/braintree-jsdoc-template"
    }
}